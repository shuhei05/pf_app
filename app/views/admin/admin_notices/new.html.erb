<div class="container">
  <%#= form_for(@admin_notice) do |f| %>
  <%#= render 'layouts/error', object: f.object %><%# end %>
  <h3>共有事項作成</h3>
  <div class="row">
    <div class="form-group ml-3" style="width:50rem;">
        <%= form_with model: [:admin, @admin_notice], local:true do |f| %>
         <label for="information_title">タイトル</label><br>
         <%= f.text_field :information_title,class:'col-9' %><br>
         <label for="information_body">内容</label><br>
         <%= f.text_area :information_body,class:'col-9' %><br>
         <%= f.submit '送信', class:'btn btn-info' %>
         <% end %>
    </div>
  </div>
</div>